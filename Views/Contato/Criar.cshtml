@{
    ViewData["Title"] = "Criar contatos";
}
<div class="text-center">

    <h1 class="display-3">Cadastrar contato</h1>

</div>

<form>
    <div class="form-group">
        <label for="nome">Nome</label>
        <input type="text" class="form-control" placeholder="Nome">
    </div>
    <div class="form-group">
        <label for="email">E-mail</label>
        <input type="email" class="form-control" placeholder="E-mail">
    </div>
    <div class="form-group">
        <label for="celular">Contato</label>
        <input type="password" class="form-control" placeholder="Digite o número do contato">
    </div>
    <br />
    <button type="submit" class="btn btn-outline-primary">Salvar</button>
    <a class="btn btn-outline-secondary" asp-controller="Contato" asp-action="Index" role="button">Voltar</a>
</form>

